generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  fullName   String                       @db.VarChar(30)
  id         Int      @id @default(autoincrement())
  password   String
  email      String   @unique             @db.VarChar(50)
  createdat  DateTime @default(now())     @db.Timestamp(6)
  ip         String                       @db.Text                
  ua         String                       @db.Text
}
